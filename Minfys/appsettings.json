{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Debug",
      "Serilog.Sinks.File"
    ],
    "WriteTo": [
      {
        "Name": "Debug"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 5
        }
      }
    ]
  },
  "AudioOptions": {
    "LoopEnabled": false,
    "FilePath": "C://Programming projects//C#//Projects//Minfys//Minfys//Assets//Audio//default_song.mp3"
  }
}